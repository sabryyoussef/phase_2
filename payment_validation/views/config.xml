<odoo>
    <data>
        <record id="deduction_settings_view" model="ir.ui.view">
            <field name="name">Deductions</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">
                    <app data-string="Operations" string="Operations" name="multiproject_saleorder">
                        <block title="Operations" name="operations_setting_container">
                            <setting id="operation_team_setting">
                                <label for="operation_team_ids" string="Operation Team" />
                                <div class="content-group">
                                    <div class="mt16">
                                        <field name="operation_team_ids"
                                            class="o_field_many2manytags" widget="many2many_tags" />
                                    </div>
                                </div>
                            </setting>
                        </block>
                    </app>
                </xpath>
            </field>
        </record>

    </data>
</odoo>